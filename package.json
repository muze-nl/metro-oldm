{
  "name": "@muze-nl/metro-oldm",
  "version": "0.1.0",
  "description": "linked data middleware for @muze-nl/metro",
  "type": "module",
  "source": [
    "src/index.mjs"
  ],
  "main": "src/index.mjs",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tap test/*.mjs",
    "tap": "tap",
    "build": "./node_modules/.bin/esbuild src/index.mjs --outfile=dist/browser.min.js --bundle --minify --sourcemap",
    "build-dev": "./node_modules/.bin/esbuild src/index.mjs --outfile=dist/browser.js --bundle --sourcemap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muze-nl/metro-oldm.git"
  },
  "author": "auke@muze.nl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/muze-nl/metro-oldm/issues"
  },
  "homepage": "https://github.com/muze-nl/metro-oldm/#readme",
  "devDependencies": {
    "esbuild": "0.20.2",
    "eslint": "^8.13.0",
    "tap": "^16.0.1"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "/src",
    "/dist"
  ],
  "dependencies": {
    "@muze-nl/oldm": "^0.3.5",
    "esbuild": "^0.27.2"
  },
  "targets": {
    "default": {
      "distDir": "./dist",
      "optimize": true
    },
    "main": false
  }
}
